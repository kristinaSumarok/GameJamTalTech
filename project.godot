; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameJamProject"
run/main_scene="res://scenes/First.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")

[autoload]

SceneLoader="*res://scripts/SceneLoader.cs"
GameState="res://scripts/game_state.cs"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogue_manager]

general/uses_dotnet=true

[display]

window/size/mode=3
window/size/initial_position_type=0
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="GameJamProject"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[input]

ui_accept={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":69,"physical_keycode":0,"key_label":0,"unicode":101,"location":0,"echo":false,"script":null)
]
}

[internationalization]

locale/translations_pot_files=PackedStringArray("res://dialogue/main.dialogue")

[layer_names]

2d_physics/layer_5="actionables"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
